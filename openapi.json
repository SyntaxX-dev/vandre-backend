              "application/json": {
                "schema": {
                  "example": {
                    "id": "1",
                    "name": "John Doe",
                    "email": "john.doe@example.com",
                    "password": "123456",
                    "created_at": "2024-02-23T10:00:00.000Z",
                    "updated_at": "2024-02-23T10:00:00.000Z"
